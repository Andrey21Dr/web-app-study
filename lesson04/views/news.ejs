<html>
    <head>
        <title>News</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
    </head>
    <body>
        <h1>News Page</h1>
        
        <ul>
        <% content.forEach(function(content) { %>
            <ol><h5><%= content.title %></h5></ol>
            <ol><%= content.content %></ol>
            <br>
        <% }); %>
        </ul>
    </body>
</html>